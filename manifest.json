{
  "name": "Wordle Solver",
  "description": "Gives potential solutions for a wordle puzzle.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  },
  "options_page": "options.html"
}
